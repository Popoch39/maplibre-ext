{
  "name": "maplibre-ext",
  "version": "0.0.1",
  "description": "A demo package for Total TypeScript",
  "keywords": [
    "demo",
    "typescript"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "homepage": "https://github.com/Popoch39/maplibre-ext",
  "bugs": {
    "url": "https://github.com/Popoch39/maplibre-ext/issues"
  },
  "author": "Louis Pocheron",
  "repository": {
    "type": "git",
    "url": "https://github.com/Popoch39/maplibre-ext"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "build": "tsup",
    "ci": "npm run format && npm run check-format && npm run build && npm run check-exports && npm run lint && npm run test",
    "lint": "tsc",
    "test": "vitest run",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack .",
    "local-release": "npm run ci && changeset version && changeset publish"
  }
}
